{
  "name": "greenfi-fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsc && vite",
    "build": "tsc && vite build",
    "build:uat": "tsc && vite build --mode uat",
    "build:stag": "tsc && vite build --mode stag",
    "build:prod": "tsc && vite build --mode prod",
    "preview": "vite preview",
    "lint": "eslint . --max-warnings 178",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "deploy": "aws s3 sync dist/ s3://<YOUR_BUCKET_NAME>",
    "uat": "npm run build:uat && npm run deploy",
    "stag": "npm run build:stag && npm run deploy",
    "prod": "npm run build:prod && npm run deploy"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.8.0",
    "@cyntler/react-doc-viewer": "^1.17.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.1.1",
    "@react-google-maps/api": "^2.19.2",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/lodash": "^4.17.15",
    "@types/react-date-range": "^1.4.9",
    "@types/react-datepicker": "^4.19.3",
    "@types/react-virtualized-auto-sizer": "^1.0.8",
    "@types/react-window": "^1.8.8",
    "@uidotdev/usehooks": "^2.2.0",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "docx": "^9.3.0",
    "file-saver": "^2.0.5",
    "firebase": "^10.3.0",
    "framer-motion": "^10.13.0",
    "json2csv": "^6.0.0-alpha.2",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.31",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.523.0",
    "mammoth": "^1.9.0",
    "moment": "^2.29.4",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-date-range": "^1.4.0",
    "react-datepicker": "^4.23.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.45.2",
    "react-icons": "^4.10.1",
    "react-leaflet": "^4.2.1",
    "react-paginate": "^8.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.14.2",
    "react-select": "^5.8.3",
    "react-select-async-paginate": "^0.7.3",
    "react-to-print": "^2.15.1",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "recharts": "^2.7.2",
    "redux-persist": "^6.0.0",
    "typescript": "^5.2.2",
    "xlsx": "^0.18.5",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@types/file-saver": "^2.0.7",
    "@types/leaflet": "^1.9.8",
    "@types/papaparse": "^5.3.11",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "prettier": "^3.1.1",
    "vite": "^4.4.5"
  }
}
